<body class="text-center">


    <main class="form-signin">
        <h3>REGISTER HERE!!</h1>
        <form action="/register" method="post">
            <h1 class="h3 mb-3 fw-normal"
                style="font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;">
                <span class="material-icons">
                    account_circle
                </span>
                <span class="material-icons">
                    visibility
                </span>
            </h1>

            <div class="form-floating">
                <input type="email" class="form-control" id="floatingInput" placeholder="name@example.com" name="username" autocomplete="off">
                <label for="floatingInput">Email address</label>
            </div>
            <div class="form-floating">
                <input type="password" class="form-control" id="floatingPassword" placeholder="Password" name="password" autocomplete="off">
                <label for="floatingPassword">Password</label>
            </div>

            <button class="w-100 btn btn-lg btn-outline-dark" type="submit">SUBMIT</button>
        </form>

    </main>

